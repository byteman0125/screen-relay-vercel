{
  "version": 2,
  "functions": {
    "api/socket.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/socket.io/(.*)",
      "dest": "/api/socket.js"
    },
    {
      "src": "/stats",
      "dest": "/api/socket.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/socket.js"
    }
  ]
}
