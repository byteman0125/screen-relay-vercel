{
  "version": 2,
  "functions": {
    "api/relay.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/register-tester",
      "dest": "/api/relay.js"
    },
    {
      "src": "/register-supporter", 
      "dest": "/api/relay.js"
    },
    {
      "src": "/send-screen",
      "dest": "/api/relay.js"
    },
    {
      "src": "/get-screen",
      "dest": "/api/relay.js"
    },
    {
      "src": "/heartbeat",
      "dest": "/api/relay.js"
    },
    {
      "src": "/stats",
      "dest": "/api/relay.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/relay.js"
    }
  ]
}
